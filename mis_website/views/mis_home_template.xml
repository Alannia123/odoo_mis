                <style>
                    .btn_effect {
                        position : relative;
                        display : inline-block;
                        font-size : 1em;
                        letter-spacing : 0.1em;
                        color : #fff;
                        text-transform : uppercase;
                        padding : 10px 30px;
                        background : #00b9fc;
                        transition : all 0.3s ease;
                        z-index : 1;
                        box-shadow : 0 0 5px #00b9fc, 0 0 8px #00b9fc;
                        line-height : 25px;
                        border : none;
                    }
                    .btn_effect:hover{
                            background : transparent;
                            color : #00b9fc;
                            box-shadow :
                                0 5px 3px -3px #00b9fc,
                                0 -5px 3px -3px #00b9fc,
                                0 5px 3px -3px #00b9fc,
                                0 -5px 3px -3px #00b9fc;
                    }
                    .btn_effect:before ,
                    btn_effect:after{
                            position : absolute;
                            content : '';
                            left : 0;
                            width : 100%;
                            height : 50%;
                            right : 0;
                            z-index : -1;
                            background : #00b9fc;
                            box-shadow : 0 0 5px #00b9fc;
                            transition : all 0.3s ease;
                    }
                    .btn_effect:before{
                            top : 0;
                    }
                    .btn_effect:after{
                            bottom : 0;
                    }

                    .btn_effect:before ,
                    btn_effect:after{
                            height : 0;
                            background : #00b9fc;
                    }

<!--                </style>-->
<!--                <style>-->
                    .btn_effect_line {
                        position : relative;
                        display : inline-block;
                        font-size : 1em;
                        letter-spacing : 0.1em;
                        color : #fff;
                        text-transform : uppercase;
                        padding : 10px 30px;
                        font-weight : 500;
                        background : #21ebff;
                        line-height : 25px;
                        border : none;
                        transition : all 0.3s ease;
                    }
                    .btn_effect_line:hover{
                            background : transparent;
                            color : #21ebff;
                    }
                    .btn_effect_line::before ,
                    btn_effect_line::after{
                            position : absolute;
                            content : '';
                            top : 0;
                            right : 0;
                            height : 2px;
                            width : 0;
                            box-shadow : 0 0 5px #21ebff, 0 0 5px #21ebff inset;
                            transition : 400ms all ease;
                    }
                    .btn_effect_line::after{
                            right : inherit;
                            top : inherit;
                            left : 0;
                            bottom : 0;
                    }
                    .btn_effect_line:hover:before,
                    .btn_effect_line:hover:after{
                            width : 100%;
                            transition : 800ms all ease;
                    }

<!--                    Button Glow-->
                    .btn_glow_effect {
                        display : inline-block;
                        margin : 60px;
                        font-size : 24px;
                        font-weight : 500;
                        letter-spacing : 1.6px;
                        color : rgba(255, 255, 255, .4);
                        background : #111;
                        padding : 10px 30px;
                        text-decoration : none;
                        text-transform : uppercase;
                        border : 2px solid rgba(255, 255, 255, .4);
                        border-radius : 10px;
                        transition : 0.8s
                    }
                    .btn_glow_effect:hover{
                            color : #e50140;
                            text-shadow : 0 0 5px #e50140;
                            box-shadow :
                                    0 0 10px #e50140,
                                    0 0 20px #e50140,
                                    0 0 40px #e50140,
                                    0 0 80px #e50140,
                                    0 0 160px #e50140;
                            letter-spacing : 4px;
                    }




/* Squre Hover Effect */

aa {
                position : relative;
                display : inline-block;
                color : rgba(255, 255, 255, .4);
                background : #111;
                padding : 10px 30px;
                font-size : 14px;
                letter-spacing : 1.2px;
                text-decoration : none;
                text-transform : uppercase;
                transition : 0.5s
                    }

aa:hover{
                    color : #0ef;
                    text-shadow : 0 0 5px #0ef;
                    letter-spacing : 4px;
                    }

aa span{
        position : absolute;
        display : block;
        background : #0ef;
        box-shadow : 0 0 5px #0ef;
        }

aa span:nth-child(1){
        left : 0;
        top : 0;
        width : 50.5%;
        height : 2px;
        transform: scaleX(0);
        transition: transform .5s;
        transform-origin: left;


}
aa:hover span:nth-child(1)
{
    transform: scaleX(1);
    transform-origin: right;
}

aa span:nth-child(2){
        right:  0;
        bottom:  0;
        width : 50.5%;
        height : 2px;
        transform: scaleX(0);
        transition: transform .5s;
        transform-origin: right;


}
aa:hover span:nth-child(2)
{
    transform: scaleX(1);
    transform-origin: left;
}
aa span:nth-child(3){
        right:  0;
        bottom:  0;
        width : 2px;
        height : 100%;
        transform: scaleY(0);
        transition: transform .5s;
        transform-origin: top;
        transition-delay: .4s;


}
aa:hover span:nth-child(3)
{
    transform: scaleY(1);
    transform-origin: bottom;
}
aa span:nth-child(4){
        left:  0;
        top:   0;
        width : 2px;
        height : 100%;
        transform: scaleY(0);
        transition: transform .5s;
        transform-origin: bottom;
        transition-delay: .4s;


}
aa:hover span:nth-child(4)
{
    transform: scaleY(1);
    transform-origin: top;
}
aa span:nth-child(5){
        right:  0;
        top:   0;
        width : 2px;
        height : 100%;
        transform: scaleY(0);
        transition: transform .5s;
        transform-origin: left;
        transition-delay: .8s;


}
aa:hover span:nth-child(5)
{
    transform: scaleY(1);
    transform-origin: right;
}
aa span:nth-child(6){
        left:  0;
        bottom:   0;
        width : 2px;
        height : 100%;
        transform: scaleY(0);
        transition: transform .5s;
        transform-origin: right;
        transition-delay: .8s;


}
